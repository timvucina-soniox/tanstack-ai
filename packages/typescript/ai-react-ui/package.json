{
  "name": "@tanstack/ai-react-ui",
  "version": "0.0.1",
  "description": "Headless React components for building AI chat interfaces",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "exit 0",
    "test:watch": "vitest"
  },
  "keywords": [
    "tanstack",
    "ai",
    "react",
    "chat",
    "ui",
    "headless",
    "components"
  ],
  "dependencies": {
    "highlight.js": "^11.11.1",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1"
  },
  "peerDependencies": {
    "@tanstack/ai-client": "workspace:*",
    "@tanstack/ai-react": "workspace:*",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "tsdown": "^0.15.9",
    "typescript": "^5.6.3",
    "vitest": "^4.0.13"
  },
  "files": [
    "dist"
  ]
}
